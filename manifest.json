 {
    "name": "ApiToken Generator",
    "version": "1.1",
    "description": "Retrieve token from url and parameters entered",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
      }
    }, 
    "icons": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    },
    "manifest_version": 2
  }